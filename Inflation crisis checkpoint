(df, column):\n","    Q1 = df[column].quantile(0.25)\n","    Q3 = df[column].quantile(0.75)\n","    IQR = Q3 - Q1\n","    lower_bound = Q1 - 1.5 * IQR\n","    upper_bound = Q3 + 1.5 * IQR\n","    df = df[(df[column] >= lower_bound) & (df[column] <= upper_bound)]\n","    return df"],"metadata":{"id":"68wWgvLI779L","executionInfo":{"status":"ok","timestamp":1752566887058,"user_tz":-180,"elapsed":21,"user":{"displayName":"Filbao","userId":"06534393836955986277"}}},"execution_count":29,"outputs":[]},{"cell_type":"code","source":["# Applying outlier removal for numeric columns\n","for col in numeric_cols:\n","    df = remove_outliers(df, col)\n","\n","print(\"\\nShape after outlier removal:\", df.shape)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":211},"id":"zQbffVP986jA","executionInfo":{"status":"error","timestamp":1752566887068,"user_tz":-180,"elapsed":23,"user":{"displayName":"Filbao","userId":"06534393836955986277"}},"outputId":"5ea0858e-794b-4b01-91e5-46951c1c8c46"},"execution_count":30,"outputs":[{"output_type":"error","ename":"NameError","evalue":"name 'numeric_cols' is not defined","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mNameError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m/tmp/ipython-input-30-1442712292.py\u001b[0m in \u001b[0;36m<cell line: 0>\u001b[0;34m()\u001b[0m\n\u001b[1;32m      1\u001b[0m \u001b[0;31m# Applying outlier removal for numeric columns\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m----> 2\u001b[0;31m \u001b[0;32mfor\u001b[0m \u001b[0mcol\u001b[0m \u001b[0;32min\u001b[0m \u001b[0mnumeric_cols\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0m\u001b[1;32m      3\u001b[0m     \u001b[0mdf\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mremove_outliers\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mdf\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0m