riables\n","binary_map = {'Yes': 1, 'No': 0, 'Female': 0, 'Male': 1}\n","\n","df['sex_code'] = df['sex'].map(binary_map)\n","df['trained_code'] = df['trained'].map(binary_map)\n","df['tb_services'] = df['ever_provided_tb_services'].map(binary_map)\n","df['tb_contact'] = df['personal_history/contact_of_tb_disease'].map(binary_map)\n","df['tb_knowledge'] = df['general_knowledge_on_tb_prevention'].map(binary_map)\n","df['aware_tpt'] = df['aware_of_tpt'].map(binary_map)\n","df['enrolled_tpt'] = df['enrolled__tpt'].map(binary_map)  # Target variable\n","\n","# Drop rows with missing values in selected features\n","features = ['age', 'level_of_the_facility', 'sex_code', 'trained_code',\n","            'no._of_pple_in_a_hsehold', 'tb_services', 'tb_contact',\n","            'tb_knowledge', 'aware_tpt'] # Changed 'aware_of_tpt' to 'aware_tpt'\n","target = 'enrolled_tpt'\n","\n","model_df = df[features + [target]].dropna()\n","\n","# Observed uptake rate\n","observed_rate = model_df[target].mean() * 100\n","print(f\"Observed TPT Uptake Rate: {observed_rate:.2f}%\")\n","\n","# Prepare X and y\n","X = model_df[features]\n","X = sm.add_constant(X)  # Add intercept\n","y = model_df[target]\n","\n","# Fit logistic regression\n","model = sm.Logit(y, X)\n","result = model.fit()\n","\n","# Print model summary\n","print(result.summary())\n","\n","# Predicted probabilities\n","preds = result.predict(X)\n","\n","# Predicted uptake rate\n","predicted_rate = preds.mean() * 100\n","print(f\"Predicted TPT Uptake Rate: {predicted_rate:.2f}%\")"]}]}